def grupAnagrams(strs):
    anagrams = {}
    
    for word in strs:
        key = ''.join(sorted(word)) 
        
        if key not in anagrams:
            anagrams[key] = []
        
        anagrams[key].append(word)
        
    return list(anagrams.values())

strs = ["act", "pots", "tops", "cat", "stop", "hat"]

print(grupAnagrams(strs))
