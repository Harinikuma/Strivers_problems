// Use this editor to write, compile and run your Java code online
class Solution{
    public void pattern7(int N){
        int start;
        for (int i =0;i < N;i++){
            
            if(i % 2 == 0){
                start=1;
            }
            else{
                start =0;
            }
            
            for(int j=0;j<=i;j++){
                System.out.print(start);
                start = 1 - start;
            }
            System.out.println();
        }
    }
    
}
class Main {
    public static void main(String[] args) {
        Solution s = new Solution();
        int N = 5;
        s.pattern7(N);
    }
}
